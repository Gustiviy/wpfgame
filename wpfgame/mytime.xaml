<Window x:Class="snowie.mytime"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:snowie"
        mc:Ignorable="d"
        Title="MY TIME" Height="600" Width="600" ResizeMode="NoResize">
    <Grid Opacity="0.2">
        <Grid.Effect>
            <BlurEffect Radius="30"/>
        </Grid.Effect>
        <Grid.Background>
            <ImageBrush ImageSource="/Untitled.png" Stretch="UniformToFill"/>
        </Grid.Background>
        <Label x:Name="hello_man" HorizontalAlignment="Left" Height="67" Margin="10,10,0,0" VerticalAlignment="Top" Width="193" Content="{}"/>

    </Grid>
</Window>
